{
  "name": "myth-kamenwriter_dev",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "imagemin": "node npm-scripts/optimize.mjs",
    "imagemin:gen": "node npm-scripts/generate.mjs",
    "icon:resize": "node npm-scripts/icon.mjs",
    "devsass": "sass --no-source-map src/css/style.scss dist/css/style.css",
    "prodsass": "sass --no-source-map src/css/style.scss dist/css/style.css --style=compressed",
    "devjs": "rollup -c",
    "start": "NODE_ENV=development npm-run-all clean:* devsass devjs --parallel dev:*",
    "build": "NODE_ENV=production npm-run-all clean:* prodsass devjs",
    "dev:sass": "npm run devsass -- --watch",
    "dev:js": "npm run devjs -- --watch",
    "dev:bs": "browser-sync start --config bs-config.js",
    "clean:css": "rimraf css",
    "clean:js": "rimraf js",
    "format": "prettier --write --ignore-path .gitignore './**/*.{html,mjs,js,jsx,ts,tsx,json,css,scss}'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "autoprefixer": "^10.4.13",
    "browser-sync": "^2.28.1",
    "css-declaration-sorter": "^6.3.1",
    "fs": "0.0.1-security",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "npm-install-missing": "^0.1.4",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^8.4.21",
    "postcss-cli": "^9.1.0",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.58.3",
    "sharp": "^0.30.7",
    "smoothscroll-polyfill": "^0.4.4"
  }
}
